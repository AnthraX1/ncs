<script>
	function gotopage(flag){
	if(form1.CurPg.value=='') form1.CurPg.value='1';
	var lCur=parseInt(form1.CurPg.value);

	if(flag=='1'){
	  form1.CurPg.value=lCur-1;
	}
	else{
	form1.CurPg.value=lCur+1;
	}

	form1.submit();
	}
function showmail(ccod,msgfile)
{

	var t="/pronline/Msg?FunName@ncsWebReplace&fname@ncsWebMailMonRead&plate@ncs/mailReadFrm.htm&ccode@"+ccod+"&msgfile@"+msgfile;
 
  tt=showModalDialog(t, window, 'status:no;center:yes;help:no;minimize:no;maximize:yew;border:thick;statusbar:no;dialogWidth:540px;scroll:yes;dialogHeight:600px');
}

</script>

</head>

<body bgcolor="#DEE8FA" leftmargin="0" topmargin="0" marginwidth="0" marginheight="0"  >
<form name="form1" method="post" action="/pronline/Msg">
<input type="hidden" name="CurPg" value="[#CurPg#]">
<input type="hidden" name="name" value="[#name#]">

<input type="hidden" name="FunName" value="ncsWebAccountMail">
<table width="100%"  border="0" valign="top" align="center"  cellpadding="0"  cellspacing=0 bordercolor="#DFDFDF">
	
	
	       <tr height="25"> 

          <td colspan="5" align="center" style="font-size:11pt;color:#00237e;font-weight:bold;">&nbsp;&nbsp;&nbsp; 
             [#name#]相关的邮件</td>
             <td><a href="#xxm" onclick="gotopage('1')">[#prepg#]</a>&nbsp;&nbsp;<a href="#xxm" onclick="gotopage('2')">[#next#]</a></td>
         </tr>
	
    <tr class="tableHead" bgcolor="#6382BD" id=ignore> 
    	   <td  align="center" width="100" height="18" bgcolor="#6382BD" class="bord2"> 
        <font color="#FFFFFF">&nbsp;时间</font></td>
      <td  align="center" width="100"  height="18" bgcolor="#6382BD" class="bord2"> 
        <font color="#FFFFFF">&nbsp;姓名</font></td>
      <td  align="center" width="100" height="18" bgcolor="#6382BD" class="bord2"> 
        <font color="#FFFFFF">&nbsp;上网场所</font></td>

      <td width="100" align="center" height="18" bgcolor="#6382BD" class="bord2"> 
        <font color="#FFFFFF">&nbsp;发件人</font></td>
     
      <td  width="100" align="center"  height="18" bgcolor="#6382BD" class="bord2"> 
        <font color="#FFFFFF">&nbsp;收件人</font></td>
      <td  align="center"  height="18" bgcolor="#6382BD" class="bord2"> 
        <font color="#FFFFFF">&nbsp;邮件标题</font></td>        	
    </tr>
    <!-- [#SLPB#] -->
    <tr bgcolor="#FFFFFF"  onDblClick="showmail('[#ccod#]','[#msgfile#]')"> 
    	<td    height="20" valign="bottom" class="bord1"> &nbsp;[#sdate#]
      </td>
      <td    height="20" valign="bottom" class="bord1"> &nbsp;[#dispname#]
      </td>
      <td   height="20" valign="bottom" class="bord1"> &nbsp;[#cname#]
      </td>
      <td    height="20" valign="bottom" class="bord1"> &nbsp;[#from#]
      </td>
      <td  valign="bottom" class="bord1">&nbsp;[#mto#] </td>
      <td  valign="bottom" class="bord1" >&nbsp;[#mesg#]&nbsp;&nbsp;</td>

    </tr>
    <!-- [#SLPE#] -->
   
</table>

</form>
</body>
</html>
