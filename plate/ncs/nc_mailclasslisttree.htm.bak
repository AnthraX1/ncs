<head>
<script>
  var cp='[#CurPg#]';
  var tp='[#TotPg#]';
   function gotoPage(t){
       cp = parseInt(cp) + parseInt(t);
      
       if(cp <= 0) cp = 1;
       if(cp > tp) cp = tp;
       
       form1.CurPg.value = cp;
       form1.submit();
       return;
   } 
  
    function Add()
    {
	    if(form1.gname.value=='')
	    {
	    	alert('请填写要添加的策略名称');
	    	return false;
	    	}
	    	return true;
    }
    function Mod()
    {
	    if(form1.gname.value=='')
	    {
	    	alert('请先选择要修改的策略');
	    	return false;
	    	}
	    	return true;
    }
    function Del()
    {
    var flag=0;
    	 for(var i=0;i<document.all.DataTable.rows.length;i++)
	    {
	    if(document.all.DataTable.rows[i].cells[0].childNodes[1].checked)
	    {
		    flag=1;
		    break;
	    }
	    }
	    if(flag==0)
	    {
	    	alert('请先选择要删除的策略');
	    	return false;
	    }
	    	return true;
    }
  
    function showf(id,name,status) {
        form1.sid.value=id;
        form1.name.value=name;
        form1.status.value=status;
     
    }
  
   function gotoRight(t){
       top.sbpm_right.main2.location.href=t
   }
 
   function goto(t){
       self.location.href=t
   }
      
</script>
