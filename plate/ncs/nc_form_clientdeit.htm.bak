<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.0 Transitional//EN">
<html>
<head>
<title>Untitled Document</title>
<meta http-equiv="Content-Type" content="text/html; charset=gb2312">
<META HTTP-EQUIV="Expires" CONTENT="0">
<META HTTP-EQUIV="Cache-Control" CONTENT="no-cache, must-revalidate">
<META HTTP-EQUIV="Pragma" CONTENT="no-cache">
<META HTTP-EQUIV="Jon Eyrick" CONTENT="no-cache">
<link rel="stylesheet" href="/css/css.css" type="text/css">
<script>

function checkNum(str){
return str.match(/\D/)==null}





     function Add()
    {
    if('[#codeflag#]'=='1'){
    var temp_str=form1.username.value;
	    if(!checkNum(temp_str)){
	    alert('场所代码必须由全部数字组成');
	     return false;
	    }

	    if(temp_str.length<14){
	    	alert('请将场所代码填写完整');
	    	return false;
       }
       }
	    if(form1.username.value=='')
	    {
	    	alert('请填写登陆名');
	    	return false;
	    	}
	   
	 
	    if(form1.groupid.value=='')
	    {
	    alert('请选择属组');
	    return false;
	    }
	    if(form1.dispname.value=='')
	    {
	    alert('请填写单位名称');
	    return false;
	    }	
	    	
	    	return true;
    }
	 
</script>

</head>

<BODY bgcolor="#DEE8FA" leftmargin="0" topmargin="0" marginwidth="0" marginheight="0"   >
	<form name="form1" method="post" action="/pronline/Msg">
  <input type="hidden" name="FunName" value="ncsWebClientAdd">
