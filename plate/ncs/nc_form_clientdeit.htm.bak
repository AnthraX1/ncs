                   </select>
                   </td>
                 </tr>
                
                
                
                <tr height="26">
                  <td>&nbsp;</td>
                  <td>&nbsp;</td>
                </tr>
              </table>
             </td>          
          </tr>
        </table> 
         
        </td>
   </div>       
    </tr>
    
  </table>

	<script> form1.groupid.value='[#groupid#]';
	form1.gtype.value='[#gtype#]';
