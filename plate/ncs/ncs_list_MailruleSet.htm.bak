                    <input type="text" name="msize" size="9" value="[#msize#]" maxlength="10">
               
                </tr>
                <tr height="30"> 
                  <td width="1%"> </td>
                  <td colspan="3"> 内容
                  <input type="text" name="content" size=70 maxlength=64 value="[#content#]">
