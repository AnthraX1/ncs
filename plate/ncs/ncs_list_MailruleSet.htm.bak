                   <option value="0">不限</option>
                   [#SLPB#]
                   <option value="[#id#]">[#name#]</option>
                   [#SLPE#]
                   </select>                      
                </tr>
                
                <tr height="30"> 
                  <td width="2%"> </td>
                  <td colspan="3"> 主题
