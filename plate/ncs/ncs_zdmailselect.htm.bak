<!-- Infrastructure code for the tree -->
<script>
function queren()
{
var temp_str='';
if(form1.did.checked)
temp_str=temp_str+"did,";
if(form1.uid.checked)
temp_str=temp_str+"udisp,";

if(form1.sdate.checked)
temp_str=temp_str+"sdate,";

if(form1.mailfrom.checked)
temp_str=temp_str+"mailfrom,";

if(form1.host.checked)
temp_str=temp_str+"host,";
if(form1.mailto.checked)
temp_str=temp_str+"mailto,";

if(form1.sip.checked)
temp_str=temp_str+"sip,";

if(form1.mailsub.checked)
temp_str=temp_str+"mailsub,";

if(form1.mailsize.checked)
temp_str=temp_str+"mailsize,";
if(form1.mailasize.checked)
temp_str=temp_str+"mailasize,";
if(form1.flags.checked)
temp_str=temp_str+"flags,";
dialogArguments.form1.zd.value=temp_str;
self.close();

dialogArguments.form1.submit();

}
function init()
{

var temp_zd='[#zd#]';

if(temp_zd.indexOf('did')!=-1)
form1.did.checked=true;

if(temp_zd.indexOf('udisp')!=-1)
form1.uid.checked=true;

if(temp_zd.indexOf('sdate')!=-1)
form1.sdate.checked=true;

if(temp_zd.indexOf('mailfrom')!=-1)
form1.mailfrom.checked=true;

if(temp_zd.indexOf('host')!=-1)
form1.host.checked=true;

if(temp_zd.indexOf('mailto')!=-1)
form1.mailto.checked=true;

if(temp_zd.indexOf('sip')!=-1)
form1.sip.checked=true;

if(temp_zd.indexOf('mailsub')!=-1)
form1.mailsub.checked=true;

if(temp_zd.indexOf('mailsize')!=-1)
form1.mailsize.checked=true;

if(temp_zd.indexOf('mailasize')!=-1)
form1.mailasize.checked=true;

if(temp_zd.indexOf('flags')!=-1)
form1.flags.checked=true;
}
</script>

</head>

