<html>

<head>
<title>查询</title>
<meta http-equiv="Content-Type" content="text/html; charset=gb2312">
<META HTTP-EQUIV="Expires" CONTENT="0">
<META HTTP-EQUIV="Cache-Control" CONTENT="no-cache, must-revalidate">
<META HTTP-EQUIV="Pragma" CONTENT="no-cache">
<META HTTP-EQUIV="Jon Eyrick" CONTENT="no-cache">
<link rel="stylesheet" href="/css/css.css" type="text/css">
<!-- Infrastructure code for the tree -->
<script>
function queren()
{
dialogArguments.parent.form1.sdate.value=form1.sdate.value;
dialogArguments.parent.form1.edate.value=form1.edate.value;
dialogArguments.parent.form1.did.value=form1.userid.value;
if(form1.userid.value!='')
dialogArguments.parent.form1.udname.value=form1.userid.options[form1.userid.selectedIndex].text;

form1.submit();
self.close();

}
function init()
{
form1.mesg.value=dialogArguments.form1.mesg.value;
form1.sip.value=dialogArguments.form1.sip.value;
form1.username.value=dialogArguments.form1.username.value;
if(dialogArguments.parent.form1.sdate.value!='')
form1.sdate.value=dialogArguments.parent.form1.sdate.value;

if(dialogArguments.parent.form1.edate.value!='')
form1.edate.value=dialogArguments.parent.form1.edate.value;

form1.userid.value=dialogArguments.parent.form1.did.value;
}
</script>

</head>

<body bgcolor="#DEE8FA" leftmargin="0" topmargin="0" marginheight="0"  oncontextmenu="return true" onload="init()">
  <form name="form1" method="post" action="/pronline/Msg" target="xx1">
  <input type="hidden" name="FunName" value="ncsWebNetLog">

  <input type="hidden" name="flag" value="1">
<table width="100%" border="0" align="center" cellpadding="0" cellspacing="0">

  <tr> 
    <td colspan="2"><table width="100%" height="27" border="0" align="center" cellpadding="0" cellspacing="0" background="/images/table_bg1.gif">
        <tr > 
          <td colspan="2" > <div align="center"><strong><font color="#FFFFFF">
             
              用户上网记录查询</font></strong></div></td>
        </tr>
      </table></td>
  </tr>
  <tr> 
    <td colspan="2"> 
        <br>
        <table width="95%" border="1"    bordercolorlight=#89ACED   bordercolordark=#ffffff    align="center" cellpadding="0" cellspacing="0" >
          <tr> 
            <td> 
             <table width="100%" border="0" align="center" >
                <tr>
                  <td colspan="4">&nbsp;</td>
                </tr>

          
                <tr height="35"> 
                  <td ></td>
                  <td align="right"><font color="#000000"><b><font size="2">单位名称</font></b></td>
                  <td ><select name="userid" style="width:200">
                  <option value="">全部</option>
                  [#SLPB#]
                  <option value="[#userid#]">[#dispname#]</option>
                  [#SLPE#]
                  </select></td>
                  <td >&nbsp;</td>
                </tr> 
              
                <tr height="35"> 
                  <td ></td>
                  <td align="right"> <font color="#000000"><b><font size="2">Mac地址</font></b></font></td>
                  <td > <input type="text" name="mesg" size="27"></td>
                  <td >&nbsp;&nbsp;</td>
                </tr> 
                <tr height="35"> 
                  <td ></td>
                  <td align="right"> <font color="#000000"><b><font size="2">上网时间从</font></b></font></td>
                  <td > <input type="text" name="sdate" size="27" maxlength="20" value="[#sdate#]"></td>
                  <td >(YYYY/MM/DD HH:MI)</td>
                </tr> 
               <tr height="35"> 
                  <td ></td>
                  <td align="right"> <font color="#000000"><b><font size="2">到&nbsp;&nbsp;&nbsp;&nbsp;</font></b></font></td>
                  <td > <input type="text" name="edate" size="27" maxlength="20" value="[#edate#]"></td>
                  <td >(YYYY/MM/DD HH:MI)</td>
                </tr> 
                <tr height="35"> 
                  <td ></td>
                  <td align="right"> <font color="#000000"><b><font size="2">用户IP</font></b></font></td>
                  <td > <input type="text" name="sip" size="27"></td>
                  <td >&nbsp;&nbsp; </td>
                </tr> 
                <tr height="35"> 
                  <td ></td>
                  <td align="right"> <font color="#000000"><b><font size="2">用户名</font></b></font></td>
                  <td > <input type="text" name="username" size="27"></td>
                  <td >&nbsp;&nbsp;<input name='Button' type='button' class='an3' onClick='queren()' value='确 认'> </td>
                </tr> 
                <tr height="35"> 
                  <td ></td>
                  <td align="right"> <font color="#000000"><b><font size="2">排&nbsp;&nbsp;序</font></b></font></td>
                  <td colspan="2"> <input type="radio" name="sorta" value="1">用户名<input type="radio" name="sorta" value="2">上网时间
                  <select name="ordera"><option value="1">升序</option>
                                        <option value="2">降序</option>
                  </select>
                  </td>
                  
                </tr> 
              </table>
            </td>
          </tr>          
        </table>

      </td>
  </tr>
</table>
 </form>
</body>
</html>
