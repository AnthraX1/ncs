<script>
   cp = [#CurPg#];
   tp = [#TotPg#];
   function gotoPage(t){
       cp = cp + t;
      
       if(cp <= 0) cp = 1;
       if(cp > tp) cp = tp;
           
       form1.CurPg.value = cp;
       form1.submit();
      
       return;
   }
   
   function gotoRight(did,uid){
   parent.frames.sbpm_right.frames.main.form1.cond.value='';
   parent.frames.sbpm_right.frames.main.form1.username.value='';
   parent.frames.sbpm_right.frames.main.form1.userid.value=did;
   parent.frames.sbpm_right.frames.main.form1.userid.value=did;
   parent.frames.sbpm_right.frames.main.form1.uid.value=uid;
   parent.frames.sbpm_right.frames.main.form1.submit();
  
   } 
      
</script>
