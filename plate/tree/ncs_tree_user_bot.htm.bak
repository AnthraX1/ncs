<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN">
<html>
<head>
<title>Untitled Document</title>
<meta http-equiv="Content-Type" content="text/html; charset=gb2312">

<script>
   cp = [#CurPg#];
   tp = [#TotPg#];
  
   function gotoPage(t){
       cp = cp + t;
              
       if(cp <= 0) cp = 1;
       if(cp > tp) cp = tp;
           
       form1.CurPg.value = cp;
       form1.submit();
      
       return;
   }
   
   function gotoRight(did,uid){
//   parent.frames.sbpm_right2.frames.xx1.form1.cond.value='';
//   parent.frames.sbpm_right2.frames.xx1.form1.username.value='';
   parent.frames.sbpm_right2.frames.xx1.form1.userid.value=did;
   alert(did);
   parent.frames.sbpm_right2.frames.xx1.form1.did.value=did;
   parent.frames.sbpm_right2.frames.xx1.form1.uid.value=uid;

   
   parent.frames.sbpm_right2.frames.xx1.form1.submit();
  
   }   
function init()
{
//  parent.frames.sbpm_right2.frames.xx1.form1.cond.value='';
	parent.frames.sbpm_right2.frames.xx1.form1.did.value='[#did#]';
	parent.frames.sbpm_right2.frames.xx1.form1.userid.value='[#did#]';
	parent.frames.sbpm_right2.frames.xx1.form1.uid.value='';
	if(typeof(parent.frames.sbpm_right2.frames.xx1.form1.ProId)=='object')
		parent.frames.sbpm_right2.frames.xx1.form1.ProId.value='[#did#]';
		
if(typeof(parent.frames.sbpm_right2.frames.xx1.form1.cname)=='object')
		parent.frames.sbpm_right2.frames.xx1.form1.cname.value='[#cname#]';
	

	parent.frames.sbpm_right2.frames.xx1.form1.submit();
}
</script>
