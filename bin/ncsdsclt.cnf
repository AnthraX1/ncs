Appsrv  ncs {
    DispName: "网络督察管理中心";
    DsServerIp: 127.0.0.1;      /* 目录服务器的Ip地址 */
    DsServerPort:8396;          /* 目录服务器的端口号 */
    KeySecret:OFF;              /* 口令是否加密       */
    Mykey:ncs;                  /* 密码               */
    StatusTime:60;
    KeepTime:120;
    MaxClients:2000;
    MinClients:4;
    ExtendStep:10;
    UserTimeOut:6000;
    Auth: Local;
}

