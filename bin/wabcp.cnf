# BCP输出模板描述
# 最后修改时间 2015/06/23
<BCP_PLATE name="WA_BCP" charset="GBK">
<?xml version="1.0" encoding="[#charset#]"?>
<MESSAGE>
  <DATASET name="WA_COMMON_010017" rmk="数据文件索引信息">
    <DATA>
      <DATASET name="WA_COMMON_010013" rmk="BCP文件格式信息">
      	[#SLPB01#]
      	<DATA>
          <ITEM key="I010032" val="\t" rmk="列分隔符（缺少值时默认为制表符\t）"/>
          <ITEM key="I010033" val="\n" rmk="行分隔符（缺少值时默认为换行符\n）"/>
          <ITEM key="A010004" val="[#source#]" rmk="数据集代码"/>
          <ITEM key="B050016" val="145" rmk="数据来源"/>
          <ITEM key="F010008" val="[#where#]" rmk="数据采集地"/>
          <ITEM key="I010038" val="1" rmk="数据起始行，可选项，不填写默认为第１行"/>
          <ITEM key="I010039" val="[#charset#]" rmk="可选项，默认为UTF-8，BCP文件编码格式（采用不带格式的编码方式，如：UTF-8无BOM）"/>
          <DATASET name="WA_COMMON_010014" rmk="BCP数据文件信息">
          	[#SLPB05#]
            <DATA>
              <ITEM key="H040003" val="[#filepath#]" rmk="文件路径"/>
              <ITEM key="H010020" val="[#filename#]" rmk="文件名"/>
              <ITEM key="I010034" val="[#linenum#]"  rmk="记录行数"/>
            </DATA>
            [#SLPE05#]
          </DATASET>
          <DATASET name="WA_COMMON_010015" rmk="BCP文件数据结构" fieldcnt="[#sumitem#]">
            <DATA>
                [#SLPB4#]<ITEM key="[#key#]" eng="[#eng#]" chn="[#chn#]" rmk="[#rmk#]"/>
                [#SLPE4#]
             </DATA>
          </DATASET>
        </DATA>
        [#SLPE01#]
      </DATASET>
    </DATA>
  </DATASET>
</MESSAGE>
</BCP_PLATE>

